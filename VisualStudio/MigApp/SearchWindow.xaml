                    <TextBox Name="WiFiName_Rout" Width="200" Style="{DynamicResource MaterialDesignTextBox}" Foreground="White" FontSize="16"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 10 0 0">
                    <TextBlock Text="IP" Style="{DynamicResource TextBlock_14}" Width="200" Margin="0 0 10 0" FontSize="18" VerticalAlignment="Center"/>
                    <Border Name="IPBorder_Rout" Width="200">
                        <StackPanel Orientation="Horizontal">
                            <TextBox Name="ip1_Rout" Width="42" Foreground="White" Background="Transparent" VerticalAlignment="Bottom" FontSize="16" TextAlignment="Center" TextChanged="Rout_IPcheck1" GotFocus="Rout_IPfocus" PreviewTextInput="NumOnlyIP" PreviewKeyDown="Rout_NextIP1"/>
                            <TextBlock Width="10" Text="." Foreground="White" VerticalAlignment="Bottom" TextAlignment="Center"/>
                            <TextBox Name="ip2_Rout" Width="42" Foreground="White" Background="Transparent" VerticalAlignment="Bottom" FontSize="16" TextAlignment="Center" TextChanged="Rout_IPcheck2" GotFocus="Rout_IPfocus" PreviewTextInput="NumOnlyIP" PreviewKeyDown="Rout_NextIP2"/>
                            <TextBlock Width="10" Text="." Foreground="White" VerticalAlignment="Bottom" TextAlignment="Center"/>
                            <TextBox Name="ip3_Rout" Width="42" Foreground="White" Background="Transparent" VerticalAlignment="Bottom" FontSize="16" TextAlignment="Center" TextChanged="Rout_IPcheck3" GotFocus="Rout_IPfocus" PreviewTextInput="NumOnlyIP" PreviewKeyDown="Rout_NextIP3"/>
                            <TextBlock Width="10" Text="." Foreground="White" VerticalAlignment="Bottom" TextAlignment="Center"/>
                            <TextBox Name="ip4_Rout" Width="42" Foreground="White" Background="Transparent" VerticalAlignment="Bottom" FontSize="16" TextAlignment="Center" TextChanged="Rout_IPcheck4" GotFocus="Rout_IPfocus" PreviewTextInput="NumOnlyIP" PreviewKeyDown="Rout_NextIP4"/>
                        </StackPanel>
                    </Border>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 10 0 0">
